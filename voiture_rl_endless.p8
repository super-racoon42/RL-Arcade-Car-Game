--btn

function get_context()
	return (pos-1)..car[9][1]..car[9][2]..car[9][3]
end

function best_move()
	local values = q_table[get_context()]
	
	if not values then  -- ヌう✽ vれたrifie avant d'utiliser
		return 2  -- action par dれたfaut
	end
	
	local best_action = 1
	local best_value = values[1]
	
	for i = 2,3 do
		if values[i] > best_value then
			best_value = values[i]
			best_action = i
		end
	end
	return best_action
end

function move()
	
	bm = best_move()
	
	if bm == 1 then
		p[pos-1] = 1
		p[pos] = 0
		pos -= 1
	end
	
	if bm == 3 then
		p[pos+1] = 1
		p[pos] = 0
		pos += 1
	end
	
end




function bt()

	if btnp(❎) then
		tour = 1
		♥ = 4
		pts = 0
		reset_tab(car)
		pos = 2
		cpt = 0
		lvl = 1
		v = 20
		state = 0
	end
	
end
