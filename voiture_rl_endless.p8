function win_draw()
  local msg = "-- win --"
  local x0 = 25
  local x0_shadow = x0-1
  local rainbow = {8, 9, 10, 11, 12, 14} -- couleurs vives en arc-en-ciel
  
  for i = 1, #msg do
    local char = sub(msg, i, i)
    local x = x0 + (i - 1) * 8
    local x_shadow = x0_shadow + (i - 1) * 8
    local y = 60 + sin(time() + i / 3) * 2.5

    -- dれたcalage cyclique dans le tableau rainbow
    local color_index = (flr(time() * 5 + i) % #rainbow) + 1
    local col = rainbow[color_index]
			
				print("\^w\^t" .. char, x_shadow, y, 1)
    print("\^w\^t" .. char, x, y, col)
  end
end


