function lose_draw()
  local msg = "-- game over --"
  local x0 = 5
  local x0_shadow = x0-1
  
  for i = 1, #msg do
    local char = sub(msg, i, i)
    local x = x0 + (i - 1) * 8
    local x_shadow = x0_shadow + (i - 1) * 8
    local y = 60 + sin(time() + i / 3) * 1.8
			
				print("\^w\^t" .. char, x_shadow, y, 2)
    print("\^w\^t" .. char, x, y, 1)
  end
end


