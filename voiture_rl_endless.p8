--display

--affiche un tableau 1dimension
function displayl(tab,x,y)
	for i=1, #tab do
		print(tab[i],x+10*i,y+10)
	end
end

--affiche un tableau entier
function display(tab,x,y)
	for i = 1,#tab do
		for j = 1,#tab[i] do
			print(tab[i][j],x+10*j,y+10*i,8)
		end
	end
end

function displayj(x,y)
	if blink_timer > 0 then
		-- clignote : alterne visible/invisible
		if (flr(time()*10)%2 == 0) return -- ne dessine pas (invisible une frame sur deux)
	end
	spr(006, x+10*pos, y)

end

function display_obs(x,y)
	for i = 1,9 do
		for j = 1,3 do
			if car[i][j] == 1 then
				spr(007, x+10*j, y+11*i)
			end
		end
	end
end

function hud()
	rectfill(1,1,35,23,7)
	rect(1,1,35,23,1)
	print("v: "..v, 3, 3)
	print("♥: "..♥, 2, 10)
	print("pts: "..pts, 3, 17) 

end

