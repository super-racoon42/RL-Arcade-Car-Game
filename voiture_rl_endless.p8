--logic

--create a new array
function new_tab()
	local tab = {}
	for i = 1, 3 do
		tab[i] = 0
	end
	return tab
end




--reset an array
function reset_tab(tab)
	for i = 1, 10 do
		for j = 1, 3 do
		 tab[i][j] = 0
		end
	end
end

--choose a random pattern
function random_pat()

    local nb_car = rnd(1) >= 0.5 and 2 or 1
    local tab = new_tab()

    local x = flr(rnd(3))+1
    if (nb_car == 1) then
        tab[1] = p1[x][1]
        tab[2] = p1[x][2]
        tab[3] = p1[x][3]
    else
        tab[1] = p2[x][1]
        tab[2] = p2[x][2]
        tab[3] = p2[x][3]
    end

    return tab

end

--fait avancer les obstacles
--a la prochaine ligne
function nextl(tab,x)

	for j = 1,9 do	
		tab[x][j] = tab[x-1][j]
	end
	
end

--fait avancer tous les 
--obstacles
function _next(tab)
	
	for i = 10,2,-1 do
			nextl(tab,i)
	end
	tab[1] = random_pat()
	
end

function verif()
	for i = 1, 3 do
		if (car[10][i] + p[i] == 2) return true
	end
	return false
end


--if theres a car in the last
--row, then its true
function aux_verif2()

	for i=1,3 do
		if (car[10][i] == 1) return true
	end
	return false

end

--make that the player gain
--points only if he goes through
--obstacles
function verif2()
	for i = 1, 3 do
		if (car[10][i] + p[i] == 1 and aux_verif2()) return true
	end
	return false
end

function verif3()
	for i = 1, 3 do
		if (car[4][i] + p[i] == 1) return true
	end
	return false
end

function hit()
	♥ -= 1
	pts -= 10
	blink_timer = blink_duration
end

function is_tab_empty(tab)
	for i = 1, #tab do
		for j = 1, #tab[i] do
			if tab[i][j] != 0 then return false end
		end
	end
	return true
end

