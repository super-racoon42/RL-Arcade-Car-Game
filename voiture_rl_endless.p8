function _update()
 
 bt()
 
 --100->300->700
	
	if blink_timer > 0 then
		blink_timer -= 1/30
	end


	if pts - last_speed_change >= 100 and v > 2 then
	    v -= 2
	    last_speed_change = pts
	end
--	if(pts == 100) then
--		v = 15
--		lvl = 2
--	end
--
--	if(pts == 300) then
--		v = 10
--		lvl = 3
--	end
	
	
		
	
	--running
	if (state == 0) then
		move()
	
		
		cpt += 1
		cpt = cpt % v
		if(cpt%v == 0) then
			_next(car)
			
			if tour >= 10 then 
				if(verif()) then
					if pts != 0 then 
						sfx(1)
						hit() 
					else 
						sfx(1) 
						♥ -= 1
						blink_timer = blink_duration
					end
				else 
					sfx(0)
					pts += 10
				end
			end
			
			
			
			tour += 1
		end
		
		if(♥ == 0) then 
			sfx(3)
			state = 1
		end
		
--		if(pts == 900) then
--			sfx(4)
--			state = 2
--		end
	end
	
end

